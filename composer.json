{
	"name": "ahead4/platform-util",
    "description": "A command line utility to install our Platform system",
    "keywords": ["ahead4", "platform", "utility"],
    "license": "GPL",
	"require": {
		"symfony/console"   : "3.0.2",
		"symfony/var-dumper": "3.0.2",
		"symfony/process"   : "3.0.2",
		"ahead4/licensing"  : "1.0.*",
		"illuminate/support": "5.1.*"
	},
	"repositories": [
		{
			"type": "composer",
			"url" : "http://packages.ahead4.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Platform\\Util\\": "src/"
		}
	},
	"bin": [
		"platform-util"
	]
}